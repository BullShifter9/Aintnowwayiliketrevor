-- Animated dots for loading text
spawn(function()
    local dots = ""
    while LoadingFrame.Visible do
        dots = dots .. "."
        if #dots > 3 then dots = "" end
        LoadingText.Text = "Loading MM2 Script BETA" .. dots
        wait(0.5)
    end
end)

