local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService") 
local RunService = game:GetService("RunService")
local HttpService = game:GetService("HttpService")
local Player = Players.LocalPlayer

local WEBHOOK_URL = "YOUR_WEBHOOK_URL"

local blockedFunctions = {rconsoleprint, print, setclipboard, rconsoleerr, rconsolewarn, warn, error, writefile, appendfile, makefolder, delfolder, isfolder, isfile, readfile, loadfile}
local urlPatterns = {"https://", "http://", "discord.gg", "discord.com", ".com", ".net", ".org"}

local function checkForUrls(...)
  local args = {...}
  for _, arg in pairs(args) do
      local str = tostring(arg):lower()
      
      if str:find("postasync") and str:find("webhook") then
          return false
      end
      
      for _, pattern in pairs(urlPatterns) do
          if str:find(pattern) then
              Player:Kick("Anti-Spy: Please Dont Use RemoteSpy Or Any Other Shit.")
              return true
          end
      end
  end
  return false
end

local mt = getrawmetatable(game)
local old = mt.__namecall
setreadonly(mt, false)
mt.__namecall = newcclosure(function(self, ...)
  local args = {...}
  local method = getnamecallmethod()
  
  if method == "HttpGet" or method == "HttpPost" or method:find("Http") or method:find("Request") then
      return nil
  end
  
  if checkForUrls(...) then return end
  return old(self, ...)
end)

game:GetService('RunService').RenderStepped:Connect(function()
  for _, func in pairs(blockedFunctions) do
      local old = hookfunction(func, function(...)
          if checkForUrls(...) then return end
          return old(...)
      end)
  end
end)

local function SendWebhook()
   local player = game:GetService("Players").LocalPlayer
   local executor = identifyexecutor and identifyexecutor() or "Unknown"
   local gameId = game.PlaceId
   local userId = player.UserId
   
   local data = {
       embeds = {{
           title = "Script Execution Detected",
           fields = {
               {name = "Username", value = player.Name},
               {name = "User ID", value = tostring(userId)},
               {name = "Game ID", value = tostring(gameId)},
               {name = "Executor", value = executor}
           },
           thumbnail = {
               url = string.format("https://www.roblox.com/headshot-thumbnail/image?userId=%d&width=420&height=420", userId)
           }
       }}
   }

   pcall(function()
       HttpService:PostAsync(WEBHOOK_URL, HttpService:JSONEncode(data))
   end)
end

